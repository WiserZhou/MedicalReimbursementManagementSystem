<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.example.wms.mapper.TDiagnosisProjectMapper">

    <!-- 通用查询映射结果 -->
    <resultMap id="BaseResultMap" type="com.example.wms.poji.TDiagnosisProject">
        <id column="id" property="id" />
        <result column="dia_id" property="diaId" />
        <result column="dia_name" property="diaName" />
        <result column="dia_exp_type" property="diaExpType" />
        <result column="dia_exp_level" property="diaExpLevel" />
        <result column="dia_starttime" property="diaStarttime" />
        <result column="dia_endtime" property="diaEndtime" />
        <result column="dia_valid" property="diaValid" />
        <result column="dia_max_prize" property="diaMaxPrize" />
        <result column="dia_hos_level" property="diaHosLevel" />
        <result column="dia_approvalmark" property="diaApprovalmark" />
    </resultMap>

    <!-- 通用查询结果列 -->
    <sql id="Base_Column_List">
        id, dia_id, dia_name, dia_exp_type, dia_exp_level, dia_starttime, dia_endtime, dia_valid, dia_max_prize, dia_hos_level, dia_approvalmark
    </sql>

    <select id="list" resultType="com.example.wms.poji.TDiagnosisProject">
        select *
        from t_diagnosis_project
        <where>
            <if test="diaId !=null">
                dia_id like concat('%',#{diaId},'%')
            </if>
            <if test="diaName != null">
                and dia_name like concat('%',#{diaName},'%')
            </if>


        </where>


    </select>



</mapper>
